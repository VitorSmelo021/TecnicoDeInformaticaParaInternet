	funcao inicio()
	{
		/* Faça um algoritmo que mostra todos os números impares de 2 a 100.
*/

		inteiro num = 3


		enquanto(num < 100){
		escreva("\nO valor do número: ",num)
		num = num + 2

		}









	}
}
