programa
{
	
	funcao inicio()
	{
		inteiro i = 1 ,num, maiorN = 0

		escreva("Digite 10 números \n")
		enquanto(i <= 10){
		leia(num)

		se(num > maiorN){
			maiorN = num
		}
			i++
	}

		escreva("O maior número é ",maiorN)
		












		
	}
}
